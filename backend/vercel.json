{
  "version": 2,
  "name": "clothesApi",
  "builds": [
    {
      "src": "./api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    { "source": "/v1/(.*)", "destination": "/api" },
    {
      "source": "/images/(.*)",
      "destination": "/public/$1"
    }
  ]
}
